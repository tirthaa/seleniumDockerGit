		pipeline{
			//master executor should set to 0
			agent any
			stages{
					stage("Build Jar"){
						steps{
								bat "mvn clean package -DskipTests"
							 }
						}
					stage("Build Image"){
						steps{
								bat "docker build -t mangeshkashid/mangesh-docker -f ./Docerfile.txt ."
							 }
						}
					stage("Push Image"){
						steps{
						withCredentials([usernamePassword(credentialsId: 'DockerHub', passwordVariable: 'Kashid@11', usernameVariable: 'mangeshkashid')]) {
								bat "docker login --username=${user} --password=${pass}"
								bat "docker push mangeshkashid/mangesh-docker:latest"
								}
							 }					
						}
				  	}
	
				}